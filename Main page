 Sub Dicount()
If (Sheet1.Range("M14").Value = False) Then
 Sheet6.Range("F10:G28").EntireColumn.Hidden = True
 Sheet1.Range("N14").Select
 Sheet1.Range("O14").Value = ""
  Sheet1.Range("N14").Value = ""
  With Selection.Validation
        .Delete
        .Add Type:=xlValidateInputOnly, AlertStyle:=xlValidAlertStop, Operator _
        :=xlBetween
        .IgnoreBlank = True
        .InCellDropdown = True
        .InputTitle = ""
        .ErrorTitle = ""
        .InputMessage = ""
        .ErrorMessage = ""
        .ShowInput = True
        .ShowError = True
    End With
    ActiveSheet.ClearCircles
    Sheet1.Range("N14").Value = ""
   
     'Sheet1.Range("N14").Value = ""
 
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .color = 5296274
        .TintAndShade = 0
        .PatternTintAndShade = 0
    End With
    Range("N14").Select
    Selection.ClearComments
    'Range("N10").Select
    Else
    
    Sheet1.Range("O14").Value = "%"
     Sheet1.Range("N14").Select
     Sheet6.Range("F10:G28").EntireColumn.Hidden = False
         'Sheet1.Range("N16").Value = "1"
       With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = 0
        .PatternTintAndShade = 0
    End With
    'ActiveSheet.Shapes.Range(Array("Button 66")).Select
 With Selection.Validation
        .Delete
        .Add Type:=xlValidateList, AlertStyle:=xlValidAlertStop, Operator:= _
        xlBetween, Formula1:="=Admin_page!$G$51:$G$56"
        .IgnoreBlank = True
        .InCellDropdown = True
        .InputTitle = ""
        .ErrorTitle = ""
        .InputMessage = ""
        .ErrorMessage = ""
        .ShowInput = True
        .ShowError = True
    End With
        Sheet1.Range("N14").Select
   Sheet1.Range("N14").AddComment
    Sheet1.Range("N14").Comment.Visible = False
   Sheet1.Range("N14").Comment.Text Text:="Select Discount" & Chr(10) & ""
    Sheet1.Range("N12").Select
    End If
    End Sub
       
    
    
Sub Mobileapp()
Dim resst1, resst2, a, c, d, e, f, g, resst3, resst4, resst5, resst6, resst7, resst8, resst9, resst10, resst11, resst12, resst13, resst14 As String
Dim b As Variant
'a = Application.VLookup(Sheet4.Range("F19").Value, Sheet4.Range("B51:E53").Value, 4, True)
'c = Sheet4.Range("K19").Value
c = Application.VLookup(Sheet4.Range("F19").Value, Sheet4.Range("B51:E53").Value, 3, True)
d = Sheet4.Range("F19").Value
'e = Application.VLookup(Sheet4.Range("F19").Value, Sheet4.Range("C6:D15").Value, 2, True)
'f = Application.VLookup(Sheet4.Range("F19").Value, Sheet4.Range("C6:S15").Value, 3, True)
If (Sheet4.Range("F19").Value <= 300) Then
'b = Sheet4.Range("C22").Value

a = Application.VLookup(d, Sheet4.Range("B51:E54").Value, 4, True)
Sheet6.Range("E27").Value = Round(a, 0)
Sheet6.Range("D27").Value = Round(a / 4 * 1.05, 0)
Sheet6.Range("C27").Value = Round(a / 12 * 1.1, 0)

ElseIf (Sheet4.Range("F19").Value <= 500) Then
 
a = Application.VLookup(d, Sheet4.Range("B51:E54").Value, 4, True)
Sheet6.Range("E27").Value = Round(a, 0)
Sheet6.Range("D27").Value = Round(a / 4 * 1.05, 0)
Sheet6.Range("C27").Value = Round(a / 12 * 1.1, 0)

ElseIf (Sheet4.Range("F19").Value <= 4000) Then
a = Application.VLookup(d, Sheet4.Range("B51:E54").Value, 3, True)
b = a * Sheet4.Range("F19").Value
Sheet6.Range("E27").Value = Round(b, 0)
Sheet6.Range("D27").Value = Round(b / 4 * 1.05, 0)
Sheet6.Range("C27").Value = Round(b / 12 * 1.1, 0)

ElseIf (Sheet4.Range("F19").Value > 4000) Then
a = Application.VLookup(d, Sheet4.Range("B51:E54").Value, 4, True)
Sheet6.Range("E27").Value = Round(a, 0)
Sheet6.Range("D27").Value = Round(a / 4 * 1.05, 0)
Sheet6.Range("C27").Value = Round(a / 12 * 1.1, 0)

End If
 End Sub
 
 
  
