Sub finaldiscount()
 Dim aa, bb, cc As Variant
'a = Application.VLookup(Sheet6.Range("C6").Value, Sheet4.Range("B22:P42").Value, 1, True)
aa = Sheet6.Range("C8").Value
pp = Sheet6.Range("C9").Value
bb = Sheet1.Range("N10").Value
cc = Application.VLookup(Sheet6.Range("C8").Value, Sheet4.Range("B22:P42").Value, 3, False)
dd = Application.VLookup(Sheet6.Range("C8").Value, Sheet4.Range("B22:P42").Value, 4, False)
ee = Application.VLookup(Sheet6.Range("C8").Value, Sheet4.Range("B22:P42").Value, 5, False)
ff = Application.VLookup(Sheet6.Range("C8").Value, Sheet4.Range("B22:P42").Value, 6, False)
gg = Application.VLookup(Sheet6.Range("C8").Value, Sheet4.Range("B22:P42").Value, 7, False)
hh = Application.VLookup(Sheet6.Range("C8").Value, Sheet4.Range("B22:P42").Value, 8, False)
ii = Application.VLookup(Sheet6.Range("C8").Value, Sheet4.Range("B22:P42").Value, 9, False)
jj = Application.VLookup(Sheet6.Range("C8").Value, Sheet4.Range("B22:P42").Value, 10, False)
kk = Application.VLookup(Sheet6.Range("C8").Value, Sheet4.Range("B22:P42").Value, 11, False)
ll = Application.VLookup(Sheet6.Range("C8").Value, Sheet4.Range("B22:P42").Value, 12, False)
